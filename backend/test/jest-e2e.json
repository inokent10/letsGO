{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@data-generation/data-generation": "<rootDir>/../libs/data-generation/src",
    "@data-generation/data-generation/(.*)": "<rootDir>/../libs/data-generation/src/$1",
    "interfaces/interfaces": "<rootDir>/../libs/interfaces/src",
    "interfaces/interfaces/(.*)": "<rootDir>/../libs/interfaces/src/$1"
  }
}